{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Deno: Debug Current File (Full Debug)",
      "type": "deno",
      "request": "launch",
      "program": "${file}",
      "console": "internalConsole",
      "args": [
        "--allow-all"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Deno: Run Current File (Fast Execution)",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "deno",
      "program": "${file}",
      "args": [
        "run",
        "--allow-all"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "stopOnEntry": false
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/chard_unix/job/leetcode",
      "program": "/home/chard_unix/job/leetcode/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}